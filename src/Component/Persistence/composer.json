{
    "name": "fh-platform/data-sync",
    "type": "symfony-bundle",
    "description": "FH Platform - DataSync",
    "keywords": [],
    "homepage": "https://github.com/FH-Platform",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FHPlatform\\Component\\Persistence\\": "/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FHPlatform\\Component\\Persistence\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "fico7489@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "2.*",
        "doctrine/orm": "2.*",
        "doctrine/common" : "*",
        "symfony/yaml": "7.*",
        "fh-platform/util": "dev-master",
        "fh-platform/persistence": "dev-master",
        "fh-platform/client": "dev-master",
        "fh-platform/config": "dev-master",
        "symfony/doctrine-messenger": "7.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.*",
        "phpunit/phpunit": "10.*",
        "phpstan/phpstan": "*",
        "ramsey/uuid-doctrine": "^2.0",
        "symfony/console": "^7.0"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
