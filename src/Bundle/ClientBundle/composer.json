{
    "name": "fh-platform/client",
    "type": "symfony-bundle",
    "description": "FH Platform - Config client",
    "keywords": [],
    "homepage": "https://github.com/FH-Platform",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FHPlatform\\ClientBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FHPlatform\\ClientBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "fico7489@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "2.*",
        "doctrine/orm": "2.*",
        "doctrine/common" : "*",
        "symfony/yaml": "7.*",
        "ruflin/elastica" : "*",
        "fh-platform/util": "dev-master",
        "fh-platform/config": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.*",
        "phpunit/phpunit": "10.*",
        "phpstan/phpstan": "*",
        "ramsey/uuid-doctrine": "^2.0",
        "symfony/console": "^7.0"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
