        "doctrine/doctrine-bundle": "2.*",
        "doctrine/orm": "2.*",
        "doctrine/common" : "*",
        "symfony/yaml": "7.*",
        "fh-platform/util": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.*",
        "phpunit/phpunit": "10.*",
        "phpstan/phpstan": "*",
        "ramsey/uuid-doctrine": "^2.0",
