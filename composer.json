{
    "name": "fh-platform/fh-platform",
    "type": "library",
    "description": "FH Platform - FH Platform",
    "keywords": [],
    "homepage": "https://github.com/FH-Platform",
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "fico7489@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "2.*",
        "doctrine/orm": "2.*",
        "doctrine/common" : "*",
        "symfony/yaml": "7.*",
        "fh-platform/util": "dev-master",
        "ruflin/elastica" : "7.*",
        "symfony/doctrine-messenger": "7.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.*",
        "phpunit/phpunit": "10.*",
        "phpstan/phpstan": "*",
        "ramsey/uuid-doctrine": "^2.0",
        "symfony/console": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "FHPlatform\\ConfigBundle\\": "src/Bundle/ConfigBundle/src/",
            "FHPlatform\\UtilBundle\\": "src/Bundle/UtilBundle/src/",
            "FHPlatform\\PersistenceBundle\\": "src/Bundle/PersistenceBundle/src/",
            "FHPlatform\\ClientBundle\\": "src/Bundle/ClientBundle/src/",
            "FHPlatform\\DataSyncBundle\\": "src/Bundle/DataSyncBundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FHPlatform\\ConfigBundle\\Tests\\": "src/Bundle/ConfigBundle/tests/",
            "FHPlatform\\UtilBundle\\Tests\\": "src/Bundle/UtilBundle/tests/",
            "FHPlatform\\PersistenceBundle\\Tests\\": "src/Bundle/PersistenceBundle/tests/",
            "FHPlatform\\ClientBundle\\Tests\\": "src/Bundle/ClientBundle/tests/",
            "FHPlatform\\DataSyncBundle\\Tests\\": "src/Bundle/DataSyncBundle/tests/"
        }
    }
}
